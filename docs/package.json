{
  "name": "reactor-blocks-playground",
  "version": "1.0.0",
  "description": "Playground for building Reactor-Blocks UI library",
  "main": "server.js",
  "scripts": {
    "start": "node ./www",
    "start:dev": "./node_modules/.bin/nodemon ./www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && npm run build:dev && npm run build:dist && npm run copy",
    "build:dev": "./node_modules/.bin/webpack --progress --colors",
    "build:dist": "./node_modules/.bin/webpack -p --progress --colors --bail",
    "build:watch": "./node_modules/.bin/webpack --progress --colors -w",
    "copy": "npm run copy:images",
    "copy:images": "cp -r ./client/images/. ./public/images/",
    "clean": "npm run clean:public",
    "clean:public": "rm -rf ./public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kristofmic/reactor-blocks.git"
  },
  "author": "C",
  "license": "Unlicense",
  "engines": {
    "node": ">= 6.10 < 7"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-loader": "6.4.1",
    "babel-plugin-rewire": "1.1.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-template": "6.24.1",
    "babel-types": "6.24.1",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "css-loader": "0.28.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "node-sass": "4.5.2",
    "nodemon": "1.11.0",
    "postcss-loader": "1.3.3",
    "sass-loader": "4.1.1",
    "style-loader": "0.16.1",
    "url-loader": "0.5.8",
    "webpack": "1.13.1"
  },
  "dependencies": {
    "axios": "0.16.1",
    "babel-core": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.24.1",
    "body-parser": "1.17.1",
    "compression": "1.6.2",
    "es6-promise": "4.1.0",
    "express": "4.15.2",
    "helmet": "3.5.0",
    "morgan": "1.8.1",
    "node-env-conf": "2.0.0",
    "prop-types": "15.5.8",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-router": "3.0.5",
    "react-transition-group": "1.1.2",
    "winston": "2.3.1"
  }
}

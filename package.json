{
  "name": "reactor-blocks",
  "version": "2.2.7",
  "description": "React UI component library",
  "repository": {
    "type": "git",
    "url": "https://github.com/kristofmic/reactor-blocks.git"
  },
  "main": "./package/",
  "scripts": {
    "start": "echo \"Error: no start specified\" && exit 1",
    "test": "yarn run lint && yarn run test:run",
    "test:mocha": "BABEL_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register ./test/mocha.conf.js ./test/jsdom.conf.js \"./src/**/*.spec.js\"",
    "test:run": "BABEL_ENV=test ./node_modules/.bin/istanbul cover --dir=./test/coverage -x=*.spec.js --report=cobertura --report=lcov ./node_modules/.bin/_mocha -- --compilers js:babel-core/register ./test/mocha.conf.js ./test/jsdom.conf.js \"./src/**/*.spec.js\"",
    "test:coverage": "./node_modules/.bin/istanbul check-coverage --statements 80 --branches 80 --function 80 --lines 80 ./test/coverage/**/coverage*.json",
    "build": "yarn run clean:package && yarn run build:styles && yarn run copy:styles && yarn run copy:fonts && yarn run copy:images && yarn run build:components && yarn run copy:package",
    "build:styles": "./node_modules/.bin/gulp stylesheets && ./node_modules/.bin/gulp stylesheets:min",
    "build:components": "BABEL_ENV=build babel --ignore *.spec.js ./src --out-dir ./package",
    "clean:package": "rm -rf ./package",
    "copy:package": "cp ./package.json ./README.md ./package",
    "copy:styles": "cp -R ./src/styles/partials ./package/styles",
    "copy:fonts": "cp -R ./src/styles/icons/font-awesome-4.7.0/fonts ./package/fonts",
    "copy:images": "cp -R ./src/images ./package/images",
    "lint": "yarn run stylelint && ./node_modules/.bin/eslint ./src --ext=js --ext=jsx",
    "stylelint": "./node_modules/.bin/stylelint src/**/*.scss --fix --syntax scss",
    "watch:styles": "./node_modules/.bin/gulp watch",
    "watch:components": "BABEL_ENV=build babel -w --ignore *.spec.js ./src --out-dir ./package"
  },
  "author": "C",
  "license": "Unlicense",
  "peerDependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "prop-types": "^15.5.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router": "^3.0.0",
    "react-transition-group": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-plugin-rewire": "1.1.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-template": "6.26.0",
    "babel-types": "6.26.0",
    "bootstrap": "4.0.0-beta",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "concat-stream": "1.6.0",
    "enzyme": "3.0.0",
    "file-loader": "0.11.2",
    "istanbul": "1.0.0-alpha.2",
    "jsdom": "11.2.0",
    "mocha": "3.5.3",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-router": "3.0.5",
    "react-test-renderer": "16.0.0",
    "react-transition-group": "2.2.0",
    "sinon": "4.0.0",
    "sinon-chai": "2.13.0",
    "stylelint": "8.1.1",
    "stylelint-config-standard": "17.0.0",
    "url-loader": "0.5.9"
  },
  "optionalDependencies": {
    "css-purge": "1.1.1",
    "eslint": "4.7.2",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "4.0.0",
    "gulp-css-purge": "1.0.27",
    "gulp-exit": "0.0.2",
    "gulp-rename": "1.2.2",
    "gulp-sass": "3.1.0",
    "gulp-util": "3.0.8",
    "node-sass": "4.5.3",
    "sass-module-importer": "1.4.0"
  },
  "dependencies": {}
}
{
  "name": "reactor-blocks",
  "version": "0.0.1",
  "description": "React UI component library",
  "repository": {
    "type": "git",
    "url": "https://github.com/kristofmic/reactor-blocks.git"
  },
  "main": "./package/",
  "scripts": {
    "start": "echo \"Error: no start specified\" && exit 1",
    "test": "yarn run lint && yarn run test:run",
    "test:mocha": "BABEL_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register ./test/mocha.conf.js ./test/jsdom.conf.js \"./src/**/*.spec.js\"",
    "test:run": "BABEL_ENV=test ./node_modules/.bin/istanbul cover --dir=./test/coverage -x=*.spec.js --report=cobertura --report=lcov ./node_modules/.bin/_mocha -- --compilers js:babel-core/register ./test/mocha.conf.js ./test/jsdom.conf.js \"./src/**/*.spec.js\"",
    "test:coverage": "./node_modules/.bin/istanbul check-coverage --statements 80 --branches 80 --function 80 --lines 80 ./test/coverage/**/coverage*.json",
    "build": "yarn run clean:package && yarn run build:styles && yarn run copy:styles && yarn run copy:fonts && yarn run copy:images && yarn run build:babel && yarn run copy:package",
    "build:styles": "./node_modules/.bin/gulp componentStyles && ./node_modules/.bin/gulp stylesheets:full && ./node_modules/.bin/gulp stylesheets:min",
    "build:babel": "BABEL_ENV=build babel --ignore *.spec.js ./src --out-dir ./package",
    "clean:package": "rm -rf ./package",
    "copy:package": "cp ./package.json ./README.md ./package",
    "copy:styles": "cp -R ./src/styles/partials ./package/styles",
    "copy:fonts": "cp -R ./src/styles/icons/font-awesome-4.7.0/fonts ./package/fonts",
    "copy:images": "cp -R ./src/images ./package/images",
    "lint": "./node_modules/.bin/eslint ./src --ext=js --ext=jsx",
    "watch:styles": "./node_modules/.bin/gulp watch",
    "watch:babel": "BABEL_ENV=build babel -w --ignore *.spec.js ./src --out-dir ./package"
  },
  "author": "C",
  "license": "Unlicense",
  "engines": {
    "node": ">= 6.10 < 7"
  },
  "peerDependencies": {
    "prop-types": "^15.5.0",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-router": "^3.0.0",
    "react-transition-group": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.2",
    "babel-loader": "6.4.1",
    "babel-plugin-rewire": "1.1.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-template": "6.24.1",
    "babel-types": "6.24.1",
    "bootstrap": "4.0.0-alpha.6",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "concat-stream": "1.6.0",
    "enzyme": "2.8.2",
    "file-loader": "0.11.1",
    "istanbul": "1.0.0-alpha.2",
    "jsdom": "9.12.0",
    "mocha": "3.2.0",
    "prop-types": "15.5.8",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-router": "3.0.5",
    "react-test-renderer": "15.5.4",
    "react-transition-group": "1.1.2",
    "sinon": "2.1.0",
    "sinon-chai": "2.9.0",
    "url-loader": "0.5.8"
  },
  "optionalDependencies": {
    "css-purge": "1.1.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "3.1.1",
    "gulp-css-purge": "1.0.27",
    "gulp-exit": "0.0.2",
    "gulp-rename": "1.2.2",
    "gulp-sass": "3.1.0",
    "gulp-util": "3.0.8",
    "node-sass": "4.5.2",
    "sass-module-importer": "1.4.0"
  },
  "dependencies": {}
}
